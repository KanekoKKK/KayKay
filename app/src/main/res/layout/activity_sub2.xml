        app:srcCompat="@drawable/zabuton"
        tools:layout_editor_absoluteY="640dp" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="175dp"
        android:layout_height="158dp"
        android:layout_marginEnd="298dp"
